{
  "name": "prizma",
  "version": "1.0.0",
  "description": "Bulgarian receipt scanning app with OCR and budget tracking",
  "private": true,
  "scripts": {
    "dev": "set NODE_OPTIONS=--max-old-space-size=8192 --no-deprecation --no-warnings && set UV_THREADPOOL_SIZE=1 && next dev --port 3000",
    "build": "next build",
    "build:analyze": "set ANALYZE=true && next build",
    "start": "next start",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report",
    "migrate:check": "npx dotenv-cli -e .env.local -- npx tsx scripts/check-uncategorized-count.ts",
    "migrate:fix-categories": "npx dotenv-cli -e .env.local -- npx tsx scripts/fix-uncategorized-items.ts"
  },
  "dependencies": {
    "@google-cloud/vision": "^5.3.3",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@sentry/nextjs": "^10.17.0",
    "@supabase/auth-helpers-nextjs": "^0.10.0",
    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "^2.75.1",
    "@types/node": "^24.8.1",
    "browser-image-compression": "^2.0.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv": "^17.2.2",
    "form-data": "^4.0.4",
    "lucide-react": "^0.544.0",
    "next": "^15.5.6",
    "openai": "^5.23.2",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "sharp": "^0.34.4",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "typescript": "^5.9.3",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^15.5.4",
    "@playwright/test": "^1.55.1",
    "@types/node": "^24.8.1",
    "@types/react": "npm:types-react@^19.0.0-rc.1",
    "@types/react-dom": "npm:types-react-dom@^19.0.0-rc.1",
    "@types/uuid": "^9.0.8",
    "autoprefixer": "^10.4.17",
    "eslint": "^8.56.0",
    "eslint-config-next": "^15.5.4",
    "playwright": "^1.55.1",
    "postcss": "^8.4.35",
    "supabase": "^2.40.7",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "receipt",
    "scanning",
    "ocr",
    "budget",
    "bulgarian",
    "nextjs",
    "supabase"
  ],
  "author": "shelfhero",
  "license": "MIT"
}
