# Supabase Configuration (Required)
NEXT_PUBLIC_SUPABASE_URL=your_supabase_url_here
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key_here
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key_here

# Google Cloud Vision API (Required for OCR)
GOOGLE_CLOUD_VISION_API_KEY=your_google_vision_api_key_here

# OpenAI API (Required for OCR fallback)
OPENAI_API_KEY=your_openai_api_key_here

# Sentry Error Tracking (Optional - Free Tier)
# Get your DSN from https://sentry.io after creating a project
NEXT_PUBLIC_SENTRY_DSN=https://xxx@xxx.ingest.sentry.io/xxx

# Sentry Advanced Config (Optional - For production source maps)
SENTRY_ORG=your-sentry-org
SENTRY_PROJECT=prizma
SENTRY_AUTH_TOKEN=your-sentry-auth-token

# Other Optional Settings
NODE_ENV=development
